    "def Chall_Names_List(driver, soup):\n",
    "    chall_names_list = []\n",
    "\n",
    "    titles = soup.find_all(\"div\",{\"class\":\"challenge-title\"})\n",
    "    for title in titles:\n",
    "        t=title.find('a')['href']\n",
    "        chall_names_list.append(t[13:])\n",
    "        \n",
    "    \n",
    "    return chall_names_list"
   ]
  },
