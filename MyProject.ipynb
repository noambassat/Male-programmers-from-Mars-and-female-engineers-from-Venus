    "def make_df(lang_name,url,i):\n",
    "    selectLang(lang_name,url)\n",
    "    html = driver.page_source\n",
    "    clean_data_list = data_process(html,lang_name,i)\n",
    "    df = pd.DataFrame(clean_data_list)\n",
    "    \n",
    "    return df"
   ]
